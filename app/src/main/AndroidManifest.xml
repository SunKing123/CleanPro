            android:name="com.xiaoniu.cleanking.utils.update.UpdateFileProvider"
            android:authorities="${applicationId}.updatefileprovider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/update_cache_path" />
        </provider>


        <service android:name=".ui.usercenter.activity.FloatingImageDisplayService"></service>


        <receiver android:name="com.xiaoniu.common.hotfix.listener.MyPatchListener$ScreenBroadcastReceiver" />

        <!-- 热修复回调 -->
        <service
            android:name="com.xiaoniu.cleanking.hotfix.service.HotFixResultService"
            android:exported="false" />

        <activity
            android:name="com.lzy.imagepicker.ui.ImagePreviewDelActivity"
            android:theme="@style/ImagePickerThemeFullScreen" />

        <!-- User defined. 用户自定义的广播接收器 -->
        <receiver
            android:name="com.xiaoniu.cleanking.jpush.JPushReceiver"
            android:enabled="true">
            <intent-filter>

                <!-- Required 用户注册SDK的intent -->
                <action android:name="cn.jpush.android.intent.REGISTRATION" />
                <!-- Required 用户接收SDK消息的intent -->
                <action android:name="cn.jpush.android.intent.MESSAGE_RECEIVED" />
                <!-- Required 用户接收SDK通知栏信息的intent -->
                <action android:name="cn.jpush.android.intent.NOTIFICATION_RECEIVED" />
                <!-- Required 用户打开自定义通知栏的intent -->
                <action android:name="cn.jpush.android.intent.NOTIFICATION_OPENED" />
                <!-- 接收网络变化 连接/断开 since 1.6.3 -->
                <action android:name="cn.jpush.android.intent.CONNECTION" />

                <category android:name="com.xiaoniu.cleanking" />
            </intent-filter>
        </receiver>

    </application>

</manifest>