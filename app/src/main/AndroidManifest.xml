        tools:ignore="GoogleAppIndexingWarning"
        tools:replace="android:label">
        <activity
            android:name=".ui.main.activity.SplashActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppLauncher">
        </activity>
        <activity-alias
            android:name="splash"
            android:label="@string/app_name"
            android:targetActivity=".ui.main.activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity-alias>

        <activity
            android:name=".scheme.ui.DispatcherActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppLauncher">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="cleanking" />
            </intent-filter>
        </activity>
        <activity
