<?xml version="1.0" encoding="utf-8"?>
<com.xiaoniu.cleanking.ui.main.widget.MyRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layout_root"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:visibility="visible">


    <LinearLayout
        android:id="@+id/viewt"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_marginTop="@dimen/dimen_56dp"
        android:gravity="center"
        android:orientation="horizontal"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@id/viewt"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="@id/viewt"
        app:layout_constraintTop_toTopOf="@id/viewt">

        <ImageView
            android:id="@+id/iv_dun"
            android:layout_width="47dp"
            android:layout_height="54dp"
            android:layout_marginRight="@dimen/dimen_15dp"
            android:src="@drawable/icon_dun" />

        <TextView
            android:id="@+id/tv_size"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:textColor="@color/white"
            android:textSize="55sp"
            android:visibility="visible" />

        <LinearLayout
            android:id="@+id/layout_right_content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dimen_4dp"
            android:orientation="vertical"
            android:visibility="visible">

            <TextView
                android:id="@+id/tv_gb"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/dimen_10dp"
                android:text="GB"
                android:textColor="@color/white"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tv_ql"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="垃圾已清理"
                android:textColor="@color/white"
                android:textSize="15sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/layout_right_cooling"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dimen_4dp"
            android:orientation="vertical"
            android:visibility="gone">

            <TextView
                android:id="@+id/tv_number_cool"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="成功降温2°C"
                android:textColor="@color/white"
                android:textSize="24sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dimen_7dp"
                android:alpha="0.6"
                android:text="60s后达到最佳降温效果"
                android:textColor="@color/white"
                android:textSize="15sp" />
        </LinearLayout>
    </LinearLayout>

    <RelativeLayout
        android:id="@+id/v_home_top"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        android:background="@color/colorPrimary"
        android:paddingRight="@dimen/dimen_16dp">

        <include layout="@layout/layout_home_title_normal" />

        <include layout="@layout/layout_title_xiding" />

    </RelativeLayout>

    <!--完成动画-->
    <com.xiaoniu.cleanking.widget.OperatorNestedScrollView
        android:id="@+id/layout_scroll"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/v_home_top"
        android:fadingEdgeLength="@dimen/dimen_0dp"
        android:fillViewport="true"
        android:overScrollMode="never"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="410dp"
                android:background="@drawable/home_bg"
                android:gravity="center"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_weight="20" />

                    <com.airbnb.lottie.LottieAnimationView
                        android:id="@+id/view_lottie_home"
                        android:layout_width="0dp"
                        android:layout_height="180dp"
                        android:layout_weight="60" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_weight="20" />
                </LinearLayout>

                <TextView
                    android:id="@+id/tv_clean_type"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="3dp"
                    android:layout_marginBottom="14dp"
                    android:alpha="0"
                    android:gravity="center"
                    android:text="@string/tool_home_hint"
                    android:textColor="@color/white"
                    android:textSize="15sp"
                    tools:alpha="1"
                    tools:visibility="visible" />

                <TextView
                    android:id="@+id/tv_clean_type01"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:textColor="@color/white"
                    android:textSize="15sp"
                    android:visibility="gone"
                    tools:text="手机存在大量垃圾"
                    tools:visibility="visible" />

                <!--                <RelativeLayout-->
                <!--                    android:layout_width="match_parent"-->
                <!--                    android:layout_height="wrap_content"-->
                <!--                    android:minHeight="120dp"-->
                <!--                    android:gravity="center">-->

                <com.xiaoniu.cleanking.widget.BreathTextView
                    android:id="@+id/tv_now_clean"
                    android:layout_width="146dp"
                    android:layout_height="@dimen/dimen_40dp"
                    android:layout_marginTop="@dimen/dimen_10dp"
                    android:layout_marginBottom="120dp"
                    android:background="@drawable/bg_btn_home_clean"
                    android:gravity="center"
                    android:text="@string/clean_junk"
                    android:textColor="@color/colorPrimary"
                    android:textSize="@dimen/text_secondary"
                    android:textStyle="bold"
                    android:visibility="invisible"
                    tools:visibility="visible" />
                <!--                </RelativeLayout>-->

            </LinearLayout>

            <LinearLayout
                android:id="@+id/v_recommend"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="@dimen/dimen_12dp"
                android:layout_marginTop="-100dp"
                android:layout_marginRight="@dimen/dimen_12dp"
                android:background="@drawable/bg_six"
                android:gravity="center"
                android:orientation="vertical"
                android:padding="@dimen/dimen_16dp">

                <LinearLayout
                    android:id="@+id/layout_middle_item"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/dimen_20dp"
                    android:orientation="horizontal"
                    android:weightSum="3">
                    <!--一键加速-->
                    <LinearLayout
                        android:id="@+id/text_acce"
                        android:layout_width="0px"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal">

                            <ImageView
                                android:id="@+id/iv_acc"
                                android:layout_width="@dimen/dimen_32dp"
                                android:layout_height="@dimen/dimen_32dp"
                                android:src="@drawable/icon_yjjs" />

                            <ImageView
                                android:id="@+id/iv_acc_g"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="-7dp"
                                android:layout_toRightOf="@+id/iv_acc"
                                android:src="@drawable/icon_finish_clean_g"
                                android:visibility="gone" />
                        </RelativeLayout>

                        <TextView
                            android:id="@+id/tv_acc"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginTop="@dimen/dimen_12dp"
                            android:text="@string/tool_one_key_speed"
                            android:textColor="@color/color_323232"
                            android:textSize="13sp" />
                    </LinearLayout>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:src="@drawable/icon_line_ver" />
                    <!--手机降温-->
                    <LinearLayout
                        android:id="@+id/line_jw"
                        android:layout_width="0px"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dimen_7dp"
                        android:layout_marginRight="@dimen/dimen_7dp"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="@dimen/dimen_32dp"
                            android:layout_height="@dimen/dimen_32dp"
                            android:layout_gravity="center_horizontal"
                            android:src="@drawable/icon_home_jw" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginTop="@dimen/dimen_12dp"
                            android:text="手机降温"
                            android:textColor="@color/color_323232"
                            android:textSize="13sp" />

                    </LinearLayout>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:src="@drawable/icon_line_ver" />
                    <!--微信专清-->
                    <LinearLayout
                        android:id="@+id/line_wx"
                        android:layout_width="0px"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="@dimen/dimen_32dp"
                            android:layout_height="@dimen/dimen_32dp"
                            android:layout_gravity="center_horizontal"
                            android:src="@drawable/icon_home_wx" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginTop="@dimen/dimen_12dp"
                            android:text="微信专清"
                            android:textColor="@color/color_323232"
                            android:textSize="13sp" />

                    </LinearLayout>
                </LinearLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:src="@drawable/icon_line_hor" />

                <LinearLayout
                    android:id="@+id/line_wq"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_20dp"
                    android:orientation="horizontal"
                    android:paddingBottom="@dimen/dimen_16dp"
                    android:weightSum="3">

                    <!--通知栏清理-->
                    <LinearLayout
                        android:id="@+id/line_super_power_saving"
                        android:layout_width="0px"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal">

                            <ImageView
                                android:id="@+id/iv_noti_clear"
                                android:layout_width="@dimen/dimen_32dp"
                                android:layout_height="@dimen/dimen_32dp"
                                android:src="@drawable/icon_home_qq" />

                            <ImageView
                                android:id="@+id/iv_noti_g"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="-7dp"
                                android:layout_toRightOf="@+id/iv_noti_clear"
                                android:src="@drawable/icon_finish_clean_g"
                                android:visibility="gone" />
                        </RelativeLayout>

                        <TextView
                            android:id="@+id/tv_noti_clear"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginTop="@dimen/dimen_12dp"
                            android:text="@string/tool_notification_clean"
                            android:textColor="@color/color_323232"
                            android:textSize="13sp" />

                    </LinearLayout>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:src="@drawable/icon_line_ver" />
                    <!--文件清理-->
                    <LinearLayout
                        android:id="@+id/text_wjgl"
                        android:layout_width="0px"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dimen_7dp"
                        android:layout_marginRight="@dimen/dimen_7dp"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="@dimen/dimen_32dp"
                            android:layout_height="@dimen/dimen_32dp"
                            android:layout_gravity="center_horizontal"
                            android:src="@drawable/icon_wjql" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginTop="@dimen/dimen_12dp"
                            android:text="文件清理"
                            android:textColor="@color/color_323232"
                            android:textSize="13sp" />

                    </LinearLayout>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:src="@drawable/icon_line_ver" />
                    <!--超强省电-->
                    <LinearLayout
                        android:id="@+id/line_shd"
                        android:layout_width="0px"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal">

                            <ImageView
                                android:id="@+id/iv_electricity"
                                android:layout_width="@dimen/dimen_32dp"
                                android:layout_height="@dimen/dimen_32dp"
                                android:src="@drawable/icon_power" />

                            <ImageView
                                android:id="@+id/iv_electricity_g"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="-7dp"
                                android:layout_toRightOf="@+id/iv_electricity"
                                android:src="@drawable/icon_finish_clean_g"
                                android:visibility="gone" />
                        </RelativeLayout>

                        <TextView
                            android:id="@+id/tv_electricity"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginTop="@dimen/dimen_12dp"
                            android:text="@string/tool_super_power_saving"
                            android:textColor="@color/color_323232"
                            android:textSize="13sp" />

                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>

            <!--分享图片-->
            <ImageView
                android:id="@+id/image_ad_bottom_first"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/v_recommend"
                android:layout_marginLeft="@dimen/dimen_16dp"
                android:layout_marginTop="@dimen/dimen_16dp"
                android:layout_marginRight="@dimen/dimen_16dp"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/image_ad_bottom_second"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/image_ad_bottom_first"
                android:layout_marginLeft="@dimen/dimen_16dp"
                android:layout_marginTop="@dimen/dimen_14dp"
                android:layout_marginRight="@dimen/dimen_16dp"
                android:visibility="gone" />

            <TextView
                android:id="@+id/text_more_function"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dimen_16dp"
                android:text="更多推荐"
                android:textColor="@color/color_666666"
                android:textSize="@dimen/dimen_13sp"
                android:visibility="visible" />

            <android.support.v7.widget.RecyclerView
                android:id="@+id/recycleview"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dimen_12dp"
                android:layout_marginTop="@dimen/dimen_8dp"
                android:layout_marginRight="@dimen/dimen_12dp"
                android:layout_marginBottom="@dimen/dimen_12dp" />

            <!-- no_net -->
            <LinearLayout
                android:id="@+id/v_no_net"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone">

                <RelativeLayout
                    android:id="@+id/v_game_clean"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dimen_12dp"
                    android:layout_marginTop="@dimen/dimen_4dp"
                    android:layout_marginRight="@dimen/dimen_12dp"
                    android:background="@drawable/bg_recommend"
                    android:paddingLeft="@dimen/dimen_16dp"
                    android:paddingRight="@dimen/dimen_16dp">

                    <ImageView
                        android:id="@+id/iv_quicken"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:src="@drawable/icon_game" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/dimen_12dp"
                        android:layout_toRightOf="@+id/iv_quicken"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/tv_quicken_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/game_quicken"
                            android:textColor="@color/color_404040"
                            android:textSize="@dimen/dimen_17sp" />

                        <TextView
                            android:id="@+id/tv_quicken"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dimen_5dp"
                            android:alpha="0.4"
                            android:text="@string/game_quicken_hint"
                            android:textColor="@color/color_404040"
                            android:textSize="@dimen/dimen_14sp" />
                    </LinearLayout>

                    <TextView
                        android:id="@+id/iv_speed_clean"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="@drawable/bg_shape_radio_black_13"
                        android:paddingLeft="@dimen/dimen_11dp"
                        android:paddingTop="@dimen/dimen_5dp"
                        android:paddingRight="@dimen/dimen_11dp"
                        android:paddingBottom="@dimen/dimen_5dp"
                        android:text="@string/quicken_now"
                        android:textColor="@color/color_777777"
                        android:textSize="@dimen/dimen_12sp" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/view_phone_thin"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dimen_12dp"
                    android:layout_marginRight="@dimen/dimen_12dp"
                    android:background="@drawable/bg_recommend"
                    android:paddingLeft="@dimen/dimen_16dp"
                    android:paddingTop="@dimen/dimen_12dp"
                    android:paddingRight="@dimen/dimen_16dp">

                    <ImageView
                        android:id="@+id/iv_file"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:src="@drawable/icon_file_unins" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/dimen_12dp"
                        android:layout_toRightOf="@+id/iv_file"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/tv_file_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/tool_soft_manager"
                            android:textColor="@color/color_404040"
                            android:textSize="@dimen/dimen_17sp" />

                        <TextView
                            android:id="@+id/tv_file"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dimen_5dp"
                            android:alpha="0.4"
                            android:text="@string/uninstall_software"
                            android:textColor="@color/color_404040"
                            android:textSize="@dimen/dimen_14sp" />
                    </LinearLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="@drawable/bg_shape_radio_black_13"
                        android:paddingLeft="@dimen/dimen_11dp"
                        android:paddingTop="@dimen/dimen_5dp"
                        android:paddingRight="@dimen/dimen_11dp"
                        android:paddingBottom="@dimen/dimen_5dp"
                        android:text="@string/tool_now_clean"
                        android:textColor="@color/color_777777"
                        android:textSize="@dimen/dimen_12sp" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/view_news"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dimen_12dp"
                    android:layout_marginRight="@dimen/dimen_12dp"
                    android:layout_marginBottom="@dimen/dimen_12dp"
                    android:background="@drawable/bg_recommend"
                    android:paddingLeft="@dimen/dimen_16dp"
                    android:paddingTop="@dimen/dimen_12dp"
                    android:paddingRight="@dimen/dimen_16dp">

                    <ImageView
                        android:id="@+id/iv_news"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:src="@drawable/icon_news" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/dimen_12dp"
                        android:layout_toRightOf="@+id/iv_news"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/tv_news_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/tool_top_news"
                            android:textColor="@color/color_404040"
                            android:textSize="@dimen/dimen_17sp" />

                        <TextView
                            android:id="@+id/tv_news"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dimen_5dp"
                            android:alpha="0.4"
                            android:text="@string/lock_news"
                            android:textColor="@color/color_404040"
                            android:textSize="@dimen/dimen_14sp" />
                    </LinearLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="@drawable/bg_shape_radio_black_13"
                        android:paddingLeft="@dimen/dimen_11dp"
                        android:paddingTop="@dimen/dimen_5dp"
                        android:paddingRight="@dimen/dimen_11dp"
                        android:paddingBottom="@dimen/dimen_5dp"
                        android:text="@string/lock_click"
                        android:textColor="@color/color_777777"
                        android:textSize="@dimen/dimen_12sp" />
                </RelativeLayout>
            </LinearLayout>

            <include layout="@layout/layout_feed" />

        </LinearLayout>
    </com.xiaoniu.cleanking.widget.OperatorNestedScrollView>

    <RelativeLayout
        android:id="@+id/rl_risk_tips_toast"
        android:layout_width="165dp"
        android:layout_height="@dimen/dimen_41dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="@dimen/dimen_40dp"
        android:layout_marginRight="@dimen/dimen_18dp"
        android:background="@mipmap/risk_tips_toast"
        android:paddingTop="@dimen/dimen_7dp">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_horizontal|center_vertical"
            android:text="手机存在风险，及时修复"
            android:textColor="@color/white"
            android:textSize="@dimen/dimen_13sp" />

    </RelativeLayout>

</com.xiaoniu.cleanking.ui.main.widget.MyRelativeLayout>
