<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:gravity="center_horizontal">

    <ImageView
        android:id="@+id/iv_guide03_tag01"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dp_30"
        android:layout_centerHorizontal="true"
        android:src="@drawable/guide03_tag01" />
    <ImageView
        android:id="@+id/iv_guide03_tag02"
        android:layout_width="@dimen/dimen_129dp"
        android:layout_height="@dimen/dp_38"
        android:layout_marginTop="18dp"
        android:layout_below="@+id/iv_guide03_tag01"
        android:layout_centerHorizontal="true"
        android:src="@drawable/guide03_tag02" />
    <ImageView
        android:id="@+id/iv_guide03_tag03"
        android:layout_width="@dimen/dimen_172dp"
        android:layout_height="@dimen/dimen_161dp"
        android:layout_below="@+id/iv_guide03_tag02"
        android:layout_alignParentRight="true"
        android:src="@drawable/guide03_tag03" />
    <ImageView
        android:id="@+id/iv_guide03_tag04"
        android:layout_width="@dimen/dimen_109dp"
        android:layout_height="@dimen/dimen_82dp"
        android:layout_below="@+id/iv_guide03_tag03"
        android:layout_marginEnd="@dimen/dimen_68dp"
        android:layout_alignEnd="@+id/iv_guide03_tag03"
        android:src="@drawable/guide03_tag04" />


</RelativeLayout>
