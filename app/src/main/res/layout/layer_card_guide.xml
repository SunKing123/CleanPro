<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/iv_guide03_tag01"
        android:layout_width="@dimen/dimen_89dp"
        android:layout_height="@dimen/dimen_89dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true" />

    <ImageView
        android:id="@+id/iv_guide02_tag02"
        android:layout_width="@dimen/dimen_80dp"
        android:layout_height="@dimen/dimen_41dp"
        android:layout_below="@+id/iv_guide02_tag01"
        android:paddingRight="@dimen/dp_25"
        android:layout_alignParentRight="true"
        android:layout_marginTop="5dp"
        android:src="@drawable/guide02_tag02" />


    <ImageView
        android:id="@+id/iv_guide02_tag03"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_below="@+id/iv_guide02_tag02"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="8dp"
        android:src="@drawable/guide02_tag03" />

    <ImageView
        android:id="@+id/iv_guide02_tag04"
        android:layout_width="@dimen/dimen_129dp"
        android:layout_height="38.5dp"
        android:layout_below="@+id/iv_guide02_tag03"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/dimen_18dp"
        android:src="@drawable/guide02_tag04" />


</RelativeLayout>
