        <shape>
            <solid android:color="#D8D8D8" />
        </shape>
    </item>
    <!--进度颜色-->
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <solid android:color="#06C581" />
            </shape>
        </clip>
    </item>

</layer-list>