<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 进度条背景色 -->
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="50dip" />
            <gradient
                android:angle="270"
                android:centerColor="#ffdce4e3"
                android:centerY="0.75"
                android:endColor="#ffdce4e3"
                android:startColor="#ffdce4e3" />
        </shape>
    </item>

    <!-- 第二进度条 -->
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="50dip" />
                <gradient
                    android:angle="270"
                    android:centerColor="#c6b7ff"
                    android:centerY="0.75"
                    android:endColor="#c3b2ff"
                    android:startColor="#b9a4ff" />
            </shape>
        </clip>
    </item>

    <!-- 第二进度条 -->
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="50dip" />
              <!--  <gradient
                    android:angle="270"
                    android:centerColor="#74ebff"
                    android:centerY="0.75"
                    android:endColor="#8eefff"
                    android:startColor="#57e8ff" />-->
                <gradient
                    android:angle="270"
                    android:endColor="#ff008f67"
                    android:startColor="#ff26ffe2"
                    android:centerY="0.75"
                    android:type="linear"
                    android:useLevel="true" />
            </shape>
        </clip>
    </item>
</layer-list>